{
    "name": "pugx/pre-execute-controller-bundle",
    "type": "symfony-bundle",
    "description": "This bundle lets you add your own filters and hooks before a controller is executed",
    "keywords": ["symfony2 controller filter domain"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Leonardo Proietti",
            "email": "leonardo.proietti@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.0"
    },
    "require-dev": {
      "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "PUGX\\PreExecuteControllerBundle": "" }
    },
    "target-dir": "PUGX/PreExecuteControllerBundle",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0"
        }
    }
}